<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_bill_product_detail"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ary.com.nld.BillProductDetail">

    <android.support.v7.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:theme="?attr/actionBarTheme"
        android:minHeight="?attr/actionBarSize"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/toolbar13" />

    <TextView
        android:text="EXP :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:id="@+id/textView22"
        android:layout_below="@+id/textView21"
        android:layout_alignStart="@+id/textView21"
        android:textAlignment="center"
        android:textSize="18sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/quantity"
        android:layout_alignBaseline="@+id/textView19"
        android:layout_alignBottom="@+id/textView19"
        android:layout_alignStart="@+id/product"
        android:textAlignment="center"
        android:layout_marginTop="28dp"
        android:textSize="18sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView21"
        android:layout_alignEnd="@+id/quantity"
        android:id="@+id/mrp"
        android:layout_marginTop="28dp"
        android:textAlignment="center"
        android:textSize="18sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView22"
        android:layout_alignEnd="@+id/mrp"
        android:id="@+id/trade"
        android:layout_marginTop="28dp"
        android:textAlignment="center"
        android:textSize="18sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView23"
        android:layout_alignEnd="@+id/trade"
        android:id="@+id/exp"
        android:layout_marginTop="28dp"
        android:textAlignment="center"
        android:textSize="18sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView23"
        android:layout_alignStart="@+id/exp"
        android:textAlignment="center"
        android:textSize="18sp"
        android:layout_marginTop="28dp"
        android:id="@+id/batch" />

    <TextView
        android:text="TRADE :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:id="@+id/textView21"
        android:textAlignment="center"
        android:textSize="18sp"
        android:layout_below="@+id/textView20"
        android:layout_alignStart="@+id/textView16" />

    <TextView
        android:text="MRP :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:id="@+id/textView20"
        android:textAlignment="center"
        android:textSize="18sp"
        android:layout_below="@+id/textView19"
        android:layout_alignStart="@+id/textView18" />

    <TextView
        android:text="BATCH NO. :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:id="@+id/textView23"
        android:textAlignment="center"
        android:textSize="18sp"
        android:layout_below="@+id/textView22"
        android:layout_alignStart="@+id/textView22" />

    <TextView
        android:text="STOCKIST :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:id="@+id/textView16"
        android:textSize="18sp"
        android:textAlignment="center"
        android:layout_below="@+id/toolbar13"
        android:layout_alignParentStart="true"
        android:layout_marginTop="28dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bill"
        android:textAlignment="center"
        android:textSize="18sp"
        android:layout_alignBaseline="@+id/textView17"
        android:layout_alignBottom="@+id/textView17"
        android:layout_toEndOf="@+id/quantity"
        android:textStyle="normal|bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="92dp"
        android:id="@+id/stockist"
        android:textSize="18sp"
        android:textAlignment="center"
        android:layout_alignBaseline="@+id/textView16"
        android:layout_alignBottom="@+id/textView16"
        android:layout_toEndOf="@+id/textView23" />

    <TextView
        android:text="QUANTITY :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView19"
        android:textSize="18sp"
        android:textAlignment="center"
        android:layout_centerVertical="true"
        android:layout_alignStart="@+id/textView20" />

    <TextView
        android:text="BILL NO. :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView17"
        android:textSize="18sp"
        android:textAlignment="center"
        android:layout_above="@+id/textView18"
        android:layout_alignStart="@+id/textView16"
        android:layout_marginBottom="45dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/product"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textStyle="normal|bold"
        android:layout_above="@+id/quantity"
        android:layout_alignStart="@+id/stockist"
        android:layout_marginBottom="25dp" />

    <TextView
        android:text="PRODUCT :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView18"
        android:textSize="18sp"
        android:textAlignment="center"
        android:layout_alignBaseline="@+id/product"
        android:layout_alignBottom="@+id/product"
        android:layout_alignStart="@+id/textView17" />


</RelativeLayout>
